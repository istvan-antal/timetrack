{
  "name": "timetrack",
  "version": "1.0.1",
  "description": "Time tracker application.",
  "main": "./src",
  "scripts": {
    "start": "bash ./scripts/start.sh",
    "run": "electron dist/main.js",
    "package": "electron-packager . TimeTrack --platform darwin --out build --arch=x64 --icon=icon.icns --overwrite --ignore=typings",
    "build": "react-ts-runtime build; tsc src/main.ts --lib es5,es6,es7,dom --outDir dist",
    "test": "tslint --project .",
    "hooks": "bash ./scripts/hooks.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "time",
    "track"
  ],
  "author": "István Miklós Antal <istvan.m.antal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "dependencies": {
    "moment": "^2.22.1",
    "photon": "git+https://github.com/connors/photon.git#master",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-ts-runtime": "0.0.12-alpha1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.20",
    "electron": "^2.0.1",
    "electron-packager": "^12.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
